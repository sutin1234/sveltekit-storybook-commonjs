{
  "name": "sveltekit-storybook",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "package": "vite package",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "storybook": "storybook dev -p 6006",
    "storybook:migrate": "npx storybook@next automigrate",
    "build-storybook": "storybook-builder-vite"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@storybook/addon-actions": "^7.0.0-beta.13",
    "@storybook/addon-docs": "^7.0.0-beta.13",
    "@storybook/addon-essentials": "^7.0.0-beta.13",
    "@storybook/addon-links": "^7.0.0-beta.13",
    "@storybook/addon-svelte-csf": "^3.0.0-next.3",
    "@storybook/preview-web": "^7.0.0-beta.13",
    "@storybook/svelte": "^7.0.0-beta.13",
    "@storybook/sveltekit": "^7.0.0-beta.13",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "babel-loader": "^9.1.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-storybook": "^0.6.8",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.1",
    "prettier-plugin-svelte": "^2.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.0.0-beta.13",
    "svelte": "^3.55.0",
    "svelte-check": "^2.10.3",
    "svelte-loader": "^3.1.4",
    "svelte-preprocess": "^5.0.0",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vite-plugin-windicss": "^1.8.10",
    "windicss": "^3.5.6"
  },
  "type": "module",
  "dependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@lukeed/uuid": "^2.0.0",
    "cookie": "^0.5.0",
    "vite": "^4.0.2"
  }
}
